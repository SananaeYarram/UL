local function GYPR_fake_script() -- intro.LocalScript 
	local script = Instance.new('LocalScript', intro)

	wait()
	script.Parent.req.Visible = true
	wait(2)
	for i = 1, 10 do
		script.Parent.ejderya.ImageTransparency = 1-(i/10)
		wait()
	end
	wait(1)
	for i = 1, 10 do
		script.Parent.ejderya.ImageTransparency = i/10
		wait()
	end
	wait(1.3)
	script.Parent.req:TweenSize(UDim2.new(0, 5,0, 241))
	script.Parent.req:TweenPosition(UDim2.new(0.481, 0,0.33, 0))
	wait(1.7)
	script.Parent.req:TweenSize(UDim2.new(0, 380,0, 241))
	script.Parent.req:TweenPosition(UDim2.new(0.28, 0,0.305, 0))
	script.Parent.req.dragon:TweenSize(UDim2.new(0, 100,0, 100))
	script.Parent.req.loading:TweenSize(UDim2.new(0, 27,0, 17))
	script.Parent.req.texts:TweenSize(UDim2.new(0, 380,0, 17))
	script.Parent.req.ultron:TweenSize(UDim2.new(0, 200,0, 50))
	script.Parent.req.wellcom:TweenSize(UDim2.new(0, 200,0, 50))
	script.Parent.req.wellcom.TextTransparency = 0
	script.Parent.req.dragon:TweenPosition(UDim2.new(0.071, 0,0.249, 0))
	script.Parent.req.loading:TweenPosition(UDim2.new(0, 0,0.929, 0))
	script.Parent.req.texts:TweenPosition(UDim2.new(0, 0,0.859, 0))
	script.Parent.req.ultron:TweenPosition(UDim2.new(0.384, 0,0.498, 0))
	script.Parent.req.wellcom:TweenPosition(UDim2.new(0.384, 0,0.29, 0))
	wait(2)
	script.Parent.req.texts.TextTransparency = 0
	wait(4)
	script.Parent.req.loading:TweenSize(UDim2.new(0, 91,0, 17))
	wait(4)
	script.Parent.req.loading:TweenSize(UDim2.new(0, 183,0, 17))
	wait(5)
	script.Parent.req.loading:TweenSize(UDim2.new(0, 267,0, 17))
	wait(4)
	script.Parent.req.loading:TweenSize(UDim2.new(0, 333,0, 17))
	wait(5)
	script.Parent.req.loading:TweenSize(UDim2.new(0, 380,0, 17))
	wait(3)
	script.Parent.req.dragon:TweenSize(UDim2.new(0, 0,0, 0))
	script.Parent.req.loading:TweenSize(UDim2.new(0, 0,0, 0))
	script.Parent.req.texts:TweenSize(UDim2.new(0, 0,0, 0))
	script.Parent.req.ultron:TweenSize(UDim2.new(0, 0,0, 0))
	script.Parent.req.wellcom:TweenSize(UDim2.new(0, 0,0, 0))
	script.Parent.req.wellcom.TextTransparency = 1
	script.Parent.req.texts.TextTransparency = 1
	script.Parent.req:TweenSize(UDim2.new(0, 5,0, 241))
	script.Parent.req:TweenPosition(UDim2.new(0.481, 0,0.33, 0))
	wait(1)
	script.Parent.req:TweenPosition(UDim2.new(0.49, 0,0.533, 0))
	script.Parent.req:TweenSize(UDim2.new(0, 0,0, 0))
	wait(1)
	for i = 1, 10 do
		script.Parent.ejderya.ImageTransparency = 1-(i/10)
		wait()
	end
	wait(1)
	for i = 1, 10 do
		script.Parent.ejderya.ImageTransparency = i/10
		wait()
	end
	
	
end
coroutine.wrap(GYPR_fake_script)()
local function NKSQ_fake_script() -- ejderya.Make GUI Draggable 
	local script = Instance.new('LocalScript', ejderya)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(NKSQ_fake_script)()
