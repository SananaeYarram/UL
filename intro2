-- Scripts:

local function POVQY_fake_script() -- leakedgames.LocalScript 
	local script = Instance.new('LocalScript', leakedgames)

	wait(3)
	for i = 1, 10 do
		script.Parent.mein.BackgroundTransparency = 1-(i/10)
		script.Parent.mein.zetyu.BackgroundTransparency = 1-(i/10)
		script.Parent.mein.Loadstringopen.BackgroundTransparency = 1-(i/10)
		script.Parent.mein.zetyu.close.BackgroundTransparency = 1-(i/10)
		script.Parent.mein.zetyu.serius.ImageTransparency = 1-(i/10)
		script.Parent.mein.zetyu.close.TextTransparency = 1-(i/10)
		script.Parent.mein.Loadstringopen.TextTransparency = 1-(i/10)
		wait()
	end

	script.Parent.mein.zetyu.close.MouseButton1Down:Connect(function()
		for i = 1, 10 do
			script.Parent.mein.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.BackgroundTransparency = i/10
			script.Parent.mein.Loadstringopen.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.close.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.serius.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.serius.ImageTransparency = i/10
			script.Parent.mein.zetyu.close.TextTransparency = i/10
			script.Parent.mein.Loadstringopen.TextTransparency = i/10
			wait()
		end
	end)

	script.Parent.mein.Loadstringopen.MouseButton1Down:Connect(function()
		for i = 1, 10 do
			script.Parent.mein.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.BackgroundTransparency = i/10
			script.Parent.mein.Loadstringopen.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.close.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.serius.BackgroundTransparency = i/10
			script.Parent.mein.zetyu.serius.ImageTransparency = i/10
			script.Parent.mein.zetyu.close.TextTransparency = i/10
			script.Parent.mein.Loadstringopen.TextTransparency = i/10
			wait()
		end
	end)



	
end
coroutine.wrap(POVQY_fake_script)()
local function CJPML_fake_script() -- Loadstringopen.RainbowText 
	local script = Instance.new('Script', Loadstringopen)

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(k/255,0/255,0/255)
	k = k + add
	wait()
	end
	while true do
	k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(255/255,k/255,0/255)
	k = k + add
	wait()
	end
	k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
	k = k + add
	wait()
	end
	k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(0/255,255/255,k/255)
	k = k + add
	wait()
	end
	k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
	k = k + add
	wait()
	end
	k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(k/255,0/255,255/255)
	k = k + add
	wait()
	end
	k = 1
	while k <= 255 do
	text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
	k = k + add
	wait()
	end
	while k <= 255 do
	text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
	k = k + add
	wait()
	end
	end
end
coroutine.wrap(CJPML_fake_script)()
local function AQJA_fake_script() -- mein.Make GUI Draggable 
	local script = Instance.new('LocalScript', mein)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(AQJA_fake_script)()
